{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "type": {
            "type": "string"
        },
        "schema": {
            "type": "string"
        },
        "generator": {
            "type": "object",
            "properties": {
                "app": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
                "generated_at": {
                    "type": "string"
                }
            },
            "required": [
                "app",
                "version",
                "generated_at"
            ]
        },
        "content": {
            "type": "object",
            "properties": {
                "publication": {
                    "type": "string"
                },
                "published_at": {
                    "type": "string"
                },
                "publisher": {
                    "type": "string"
                },
                "pages": {
                    "type": "integer"
                },
                "articles": {
                    "type": "array",
                    "items": [
                        {
                            "type": "object",
                            "properties": {
                                "extracted_from": {
                                    "type": "array",
                                    "items": [
                                        {
                                            "type": "string"
                                        }
                                    ]
                                },
                                "confidence": {
                                    "type": "number"
                                },
                                "id": {
                                    "type": "integer"
                                },
                                "page": {
                                    "type": "integer"
                                },
                                "headline": {
                                    "type": "string"
                                },
                                "subhead": {
                                    "type": "string"
                                },
                                "lead": {
                                    "type": "string"
                                },
                                "byline": {
                                    "type": "object",
                                    "properties": {
                                        "name": {
                                            "type": "string"
                                        },
                                        "email": {
                                            "type": "string"
                                        }
                                    },
                                    "required": [
                                        "name",
                                    ]
                                },
                                "paragraphs": {
                                    "type": "array",
                                    "items": [
                                        {
                                            "type": "object",
                                            "properties": {
                                                "kind": {
                                                    "type": "string"
                                                },
                                                "value": {
                                                    "type": "string"
                                                }
                                            },
                                            "required": [
                                                "kind",
                                                "value"
                                            ]
                                        }
                                    ]
                                }
                            },
                            "required": [
                                "headline",
                                "paragraphs"
                            ]
                        }
                    ]
                }
            },
            "required": [
                "publication",
                "published_at",
                "publisher",
                "pages",
                "articles"
            ]
        }
    },
    "required": [
        "type",
        "schema",
        "generator",
        "content"
    ]
}